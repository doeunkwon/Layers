{
  "compilerOptions": {
    "strict": true,
		"baseUrl": ".",
    "module": "commonjs",
    "outDir": "build",
    "rootDir": "./",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "target": "es6",
    "lib": ["es6"],
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  },
  "include": ["**/*.ts", "**/*.tsx", ".eslintrc.cjs", "server.ts"],
  "exclude": [ "node_modules", "sessions" ]
}
